<!--locals: { animal }-->
<div class="custom-control custom-checkbox">
  <span class="float-right badge badge-light round"></span>

  <%= check_box_tag 'animal_ids[]', animal.id, checked(animal), :id => "animal_ids_#{animal.id}", class: 'custom-control-input', :action => "filter" %>
  <%#= link_to('jake', root_url, class: "custom-control-label") %>
<!--  <input type="checkbox" name="gender" value="female" id="female" onClick="redirect('www.google.com')">-->
  <%= label_tag "animal_ids_#{animal.id}", animal.name, class: "custom-control-label", link_to: root_url %>
</div>

