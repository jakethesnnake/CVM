<!--locals: { parameter }-->
<div class="custom-control custom-checkbox">
  <span class="float-right badge badge-light round"></span>
  <%= check_box_tag 'parameter_ids[]', parameter.id, checked(parameter), :id => "parameter_ids_#{parameter.id}", class: 'custom-control-input' %>
  <%= label_tag "parameter_ids_#{parameter.id}", parameter.name, class: "custom-control-label" %>
</div>